<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="BoardMapper">
  
  <select id="selectBoard" resultType="map">
    select 
      board_id,
      title,    
      contents,
      created_date, 
      view_count  
    from x_board
  </select>
</mapper>







